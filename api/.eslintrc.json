{
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {},
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "camelcase": 2,
    "curly": 2,
    "eqeqeq": 2,
    "no-extend-native": 2,
    "indent": [
      2,
      4,
      {
        "SwitchCase": 0
      }
    ],
    "new-cap": 2,
    "quotes": [
      2,
      "single"
    ],
    "max-depth": [
      2,
      3
    ],
    "max-statements": [
      2,
      15
    ],
    "no-eq-null": 2,
    "block-scoped-var": 2,
    "no-unused-vars": 2,
    "no-undef": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "no-use-before-define": 2,
    "no-caller": 2,
    "strict": [
      2,
      "function"
    ]
  }
}
